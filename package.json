{
  "name": "setlister",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mookybear/setlister.git"
  },
  "author": "mookybear",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "dev": "ng serve",
    "start": "node ./dist/apps/api/main.js",
    "build:all": "npm run build:ui && npm run build:universal && npm run build:api",
    "build:ui": "ng build --stats-json",
    "build:universal": "exitzero ng run ui:server",
    "build:api": "exitzero ng run api:build",
    "build:types": "concurrently -k \"npm run start\" \"wait-on http-get://localhost:3333/ && npm run graphql-codegen\"",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "analyze-bundle": "webpack-bundle-analyzer dist/apps/ui/stats-es2015.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.0",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/ng-universal": "^2.0.1",
    "@nestjs/platform-express": "^6.8.3",
    "@nestjs/platform-fastify": "^6.10.1",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-server-fastify": "^2.9.9",
    "class-transformer": "^0.2.3",
    "core-js": "^2.5.4",
    "fastify-compress": "^1.1.0",
    "fastify-plugin": "^1.6.0",
    "fastify-static": "^2.5.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "iltorb": "^2.4.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.4.0",
    "wait-on": "^3.3.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@nrwl/angular": "^8.7.1",
    "@nrwl/cypress": "8.7.1",
    "@nrwl/jest": "8.7.1",
    "@nrwl/nest": "^8.9.0",
    "@nrwl/node": "^8.9.0",
    "@nrwl/workspace": "8.7.1",
    "@types/iltorb": "^2.3.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "@types/node-cache": "^4.2.5",
    "codelyzer": "~5.0.1",
    "concurrently": "^5.0.0",
    "cypress": "3.4.1",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "exitzero": "^1.0.1",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "prettier": "1.18.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "type-graphql": "^0.17.5",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
