{
  "name": "setlister",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mookybear/setlister.git"
  },
  "author": "mookybear",
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"ui/**/*.ts\" \"api/**/*.ts\" \"model/**/*.ts\"",
    "dev": "npm run start:ui & npm run start:api:dev",
    "start:ui": "cd ui && ng serve",
    "build:ui": "cd ui && ng build",
    "build:ui:prod": "cd ui && ng build --prod",
    "lint:ui": "cd ui && ng lint",
    "prebuild": "rimraf dist",
    "build:api": "cd api && nest build",
    "start:api": "cd api && nest start",
    "start:api:dev": "cd api && nest start --watch",
    "start:api:debug": "cd api && nest start --debug --watch",
    "lint:api": "tslint -p api/tsconfig.json -c api/tslint.json",
    "test:api": "jest -p api",
    "test:api:watch": "jest -p api --watch",
    "test:api:cov": "jest -p api --coverage",
    "test:api:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:api:e2e": "jest -p api --config ./test/jest-e2e.json",
    "lint:all": "npm run lint:ui && npm run lint:api",
    "test:all": "npm run test:api:cov && npm run test:api:e2e",
    "build:all": "npm run build:ui:prod && npm run build:api",
    "ci": "npm run lint:all && npm run test:all && npm run build:all",
    "start": "node dist/api/src/main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/serve-static": "^1.0.2",
    "@types/aws-sdk": "^2.7.0",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-server-express": "^2.9.14",
    "aws-sdk": "^2.591.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "type-graphql": "^0.17.5",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "codelyzer": "^5.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "~3.4.5"
  }
}
